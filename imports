# Imports. {{{1
. "$XDG_CONFIG_HOME/shell/aliases"
. "$XDG_CONFIG_HOME/shell/exports"

z="/etc/profile.d/z.sh"
[ -f "$z" ] && . "$z"

solarized="$XDG_CONFIG_HOME/dircolors/solarized-dark"
[ -f "$solarized" ] && . "$solarized"
