# Imports. {{{1
source "$XDG_CONFIG_HOME/shell/aliases"
source "$XDG_CONFIG_HOME/shell/exports"

dircolors="$XDG_CONFIG_HOME/dircolors/dircolors"
[ -f "$dircolors" ] && source "$dircolors"

source "$XDG_CONFIG_HOME/shell/platform"
